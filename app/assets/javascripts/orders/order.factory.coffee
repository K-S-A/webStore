'use strict'

angular.module('mainApp').factory 'Order', [
  'railsResourceFactory'
  'railsSerializer'
  'localStorageService'
  '$filter'
  (railsResourceFactory, railsSerializer, localStorageService, $filter) ->
    Order = railsResourceFactory(
      url: 'orders/{{id}}'
      name: 'order'
      serializer: railsSerializer ->
        @only 'id', 'stockNumber', 'orderItems', 'comment'
        @resource 'orderItems', 'OrderItem'
        @nestedAttribute 'orderItems'
    )

    Order.beforeRequest (data) ->
      data?.order_items_attributes.forEach (item) ->
        item.product_id = item.product.id
        item.price = item.product.price
        delete item.product
      data

    Order.current = localStorageService.get('order') || {items: []}
    Order.current.created_at ||= new Date()
    Order.current.stock_number ||= 'ЭМ010012713'

    Order.getAll = ->
      Order.get().then (data) ->
        Order.all = data

    Order.find = (id) ->
      Order.get(id).then (data) ->
        Order.current = data

    Order.create = (params) ->
      new Order(params).create()

    Order.addItem = (product, quantity) ->
      Order.current.orderItems ||= []
      ids = Order.current.orderItems.map (item) ->
        item.product.id
      index = ids.indexOf(product.id)

      if index == -1
        Order.current.orderItems.push(product: product, quantity: quantity)
      else
        Order.current.orderItems[index].quantity += 1

      Order.to_lstorage('order', Order.current)

    Order.removeItem = (item) ->
      index = -1
      Order.current.orderItems.forEach (el, i) ->
        index = i if item.product.id == el.product.id

      if index > -1
        Order.current.orderItems.splice(index, 1)
        Order.to_lstorage('order', Order.current)

    Order.to_lstorage = (key, value) ->
      localStorageService.set(key, value)

    Order.total = (order) ->
      order.orderItems.reduce (a, e) ->
        a += Order.getItemTotal(e)
      , 0

    Order.getItemTotal = (item) ->
      item.quantity * (item.price || item.product.price)

    Order.toPdf = (order) ->
      pageSize: 'A4'
      pageOrientation: 'portrait'
      pageMargins: [ 10, 50, 10, 20 ]
      info:
        title: 'Счет_№397'
        author: 'Электромаркет'
        subject: 'Счет на оплату № 397 от 12 июля 2016 г.'
      header:
        text: [
          'Внимание! Оплата данного счета означает согласие с условиями поставки товара. Уведомление об оплате'
          'обязательно, в противном случае не гарантируется наличие товара на складе. Товар отпускается по факту'
          'прихода денег на р/с Поставщика, самовывозом, при наличии доверенности и паспорта.'
        ].join('\n')
        alignment: 'center'
        fontSize: 8
        margin: [0, 5]
      content: [
        {
          style: 'reqisitesMain'
          table:
            widths: [145, 145, '*', 200]
            body: [
              [
                {
                  text: [{ text: 'АО "РАЙФФАЙЗЕНБАНК" Г. МОСКВА\n\n' }, { text: 'Банк получателя', style: 'reqisitesMinor' }]
                  colSpan: 2
                  rowSpan: 2
                }
                {}
                'БИК'
                '44525700'
              ]
              [
                {}
                {}
                'Сч. №'
                '30101810200000000700'
              ]
              [
                'ИНН\t\t7728322944'
                'КПП\t\t772801001'
                { text: 'Сч. №', rowSpan: 2 }
                { text: '40702810700000013833', rowSpan: 2 }
              ]
              [
                {
                  text: [{ text: 'Общество с ограниченной ответственностью "Электромаркет"\n\n' }, { text: 'Получатель', style: 'reqisitesMinor' }]
                  colSpan: 2
                }
                {}
                {}
                {}
              ]
            ]
        }
        {
          style: 'orderTitle'
          text: Order.buildTitle(order)
        }
        { canvas: [{ type: 'line', x1: 0, y1: 0, x2: 575, y2: 0, lineWidth: 2, lineColor: 'grey' }] }
        {
          margin: 10
          columns: [
            {
              width: 90
              style: 'descDetails'
              text: 'Поставщик:'
            }
            {
              width: '*'
              style: 'orgDetails'
              text: 'Общество с ограниченной ответственностью "Электромаркет", ИНН 7728322944, КПП 772801001, 117279, г. Москва, ул. Миклухо-Маклая, д.34, оф.26, тел.: (495) 585-51-82'
            }
          ]
        }
        {
          margin: [10, 0, 10, 10]
          columns: [
            {
              width: 90
              style: 'descDetails'
              text: 'Покупатель:'
            }
            {
              width: '*'
              style: 'orgDetails'
              text: 'Общество с ограниченной ответственностью ".......", ИНН __________, КПП __________, ________, г. ______, ул. _____________________, д.__, оф.__, тел.: (___) ___-__-__'
            }
          ]
        }
        {
          style: 'orderItems'
          table:
            widths: [25, 310, 40, 25, 50, '*']
            headerRows: 1
            body: Order.buildBody(order)
        }
        {
          style: 'tableSummary'
          columns: [
            {
              width: 493
              text: 'Итого:\nСумма НДС:\nВсего к оплате:'
            }
            {
              width: '*'
              text: Order.buildResults(order)
            }
          ]
        }
        {
          style: 'receiptSummary'
          text: Order.buildSummary(order)
        }
        {
          style: 'receiptTotal'
          text: Order.buildTotal(order)
        }
        { canvas: [{ type: 'line', x1: 0, y1: 10, x2: 575, y2: 10, lineWidth: 2, lineColor: 'grey' }] }
        {
          margin: [0, 5, 0, 0]
          image: 'data:image/png;base64,'
          width: 575
        }
      ]
      styles:
        reqisitesMain:
          fontSize: 10
        reqisitesMinor:
          fontSize: 8
        orderTitle:
          fontSize: 14
          bold: true
          margin: [10, 20, 0, 10]
        orgDetails:
          fontSize: 9
          bold: true
        descDetails:
          fontSize: 9
          margin: [0, 5, 0, 0]
        orderItemsHeader:
          fontSize: 10
          alignment: 'center'
          bold: true
        orderItems:
          margin: [0, 0, 0, 10]
          fontSize: 8
        tableSummary:
          margin: [0, 0, 5, 0]
          fontSize: 10
          bold : true
          alignment: 'right'
        receiptSummary:
          fontSize: 10
        receiptTotal:
          fontSize: 10
          bold : true
          margin: [0, 5, 0, 0]

    Order.buildBody = (order) ->
      result = [[
        { text: '№', style: 'orderItemsHeader' }
        { text: 'Товары (работы, услуги)', style: 'orderItemsHeader' }
        { text: 'Кол-во', style: 'orderItemsHeader' }
        { text: 'Ед.', style: 'orderItemsHeader' }
        { text: 'Цена', style: 'orderItemsHeader' }
        { text: 'Сумма', style: 'orderItemsHeader' }
      ]]
      order.orderItems.forEach (item, i) ->
        result.push([
          { text: (i + 1).toString(), alignment: 'center' }
          { text: item.product.name }
          { text: (item.quantity).toString(), alignment: 'right' }
          { text: item.product.scu }
          { text: $filter('currency')(item.product.price, '', 2), alignment: 'right' }
          { text: $filter('currency')(Order.getItemTotal(item), '', 2), alignment: 'right' }
        ])
      result

    Order.buildResults = (order) ->
      total = Order.total(order)
      vat = $filter('currency')((total * 18 / 118.0), '', 2)
      total = $filter('currency')(total, '', 2)
      [total, vat, total].join('\n')

    Order.buildSummary = (order) ->
      total = $filter('currency')(Order.total(order), '', 2)
      'Всего наименований ' + order.orderItems.length + ', на сумму ' + total + ' руб.'

    Order.buildTotal = (order) ->
      $filter('num2word')(Order.total(order))

    Order.buildTitle = (order) ->
      'Счет на оплату № ' + order.stock_number + ' от ' + $filter('date')(order.created_at, 'd MMMM yyyy г.')

    Order
]
